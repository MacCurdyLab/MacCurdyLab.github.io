author = {Brandon Hayes , Lawrence Smith, Heiko Kabutz , Austin C. Hayes , Gregory L. Whiting , Kaushik Jayaram and Robert MacCurdy},
TITLE = {<a href = "https://www.mdpi.com/2072-666X/13/10/1634">Rapid Fabrication of Low-Cost Thermal Bubble-Driven Micro-Pumps</a>},
