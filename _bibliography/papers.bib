journal = {Journal of Visualized Experiments},
number = {February},
pages = {1--15},
pmid = {35225265},
title = {<a href="https://www.jove.com/t/63214/voxel-printing-anatomy-design-fabrication-realistic-presurgical">Voxel Printing Anatomy: Design and Fabrication of Realistic, Presurgical Planning Models through Bitmap Printing</a>},
volume = {180},
year = {2022},
pdf = {2022_Voxel_Printing_Anatomy-_Design_and_Fabrication_of_Realistic,_Presurgical_Planning_Models_through_Bitmap_Printing.pdf},
